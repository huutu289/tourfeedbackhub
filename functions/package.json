{
  "name": "tourfeedbackhub-functions",
  "version": "1.0.0",
  "description": "Cloud Functions for TourFeedbackHub",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "serve": "npm run build && firebase emulators:start --only functions",
    "shell": "npm run build && firebase functions:shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log"
  },
  "engines": {
    "node": "20"
  },
  "dependencies": {
    "firebase-admin": "^12.7.0",
    "firebase-functions": "^6.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.17.15",
    "typescript": "^5.7.3"
  },
  "private": true
}
